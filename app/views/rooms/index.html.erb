<h1>Listing rooms</h1>

<table>
  <thead>
    <tr>
      <th>Property</th>
      <th>Tenant</th>
      <th>Subletter</th>
      <th>Listing</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.property %></td>
        <td><%= room.tenant %></td>
        <td><%= room.subletter %></td>
        <td><%= room.listing %></td>
        <td><%= link_to 'Show', room %></td>
        <td><%= link_to 'Edit', edit_room_path(room) %></td>
        <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_room_path %>
