<h1>Listing rooms</h1>

<div class="room">
    <% @rooms.each do |room| %>
      <ul>
        <li><%= room.property %></li>
        <li><%= room.tenant %></li>
        <li><%= room.subletter %></li>
        <li><%= room.listing %></li>
      </ul>
    <% end %>
    <p><%= link_to 'Learn More', property_room %></p>
</div>

